{
  "expo": {
    "name": "pet_animals",
    "slug": "pet_animals",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/images/icon.png",
    "splash": {
      "image": "./app/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./app/assets/images/favicon.png"
    },
    "description": "This Project is creating an App compatible with both IOS & Android using the React-native framework under the topic Pet Animals Shop. This repo contains the Frontend of the app which is connected to the Backend using API key.",
    "githubUrl": "https://github.com/kishore007k/pet_animals"
  }
}
